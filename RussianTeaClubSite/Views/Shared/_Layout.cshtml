<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div class="navbar navbar-inverse" role="navigation">
        <a class="navbar-brand" href="#">Интернет страничка Московского клуба!</a>
    </div>
    <div class="row panel">
        <div id="categories" class="col-xs-3">
            @Html.Action("Menu", "Nav")

            <div class="thumbnail" style="margin-top: 15px">
                <iframe src="https://www.google.com/calendar/embed?height=300&amp;wkst=2&amp;bgcolor=%23ffffff&amp;src=a.kouprienko%40corvusalba.ru&amp;color=%23B1365F&amp;ctz=Europe%2FMoscow" style="border-width: 0" width="400" height="300" frameborder="0" scrolling="no"></iframe>
            </div>
        </div>
        <div class="col-xs-8">
            @RenderBody()
        </div>
    </div>
</body>
</html>